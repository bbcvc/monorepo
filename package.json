{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "pnpm run build:svg && pnpm run dev:ui & pnpm run dev:web & pnpm run dev:tanstack",
    "dev:ui": "pnpm -F ui run dev",
    "dev:web": "pnpm -F web run dev",
    "dev:tanstack": "pnpm -F tanstack run dev",
    "dev:infra": "pnpm -F infra run dev",
    "build": "turbo run build",
    "build:svg": "pnpm run -F icons build:svg",
    "format": "prettier **/*.{js,ts,jsx,tsx} --write --ignore-path .gitignore && git update-index --again"
  },
  "postinstall": "pnpm run build:svg",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "lint-staged": "^15.3.0",
    "prettier": "^3.2.1",
    "turbo": "^2.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.0.14",
    "tslib": "^2.3.0",
    "vite": "^6.2.2"
  }
