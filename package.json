{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm run build:svg && pnpm run dev:ui & pnpm run dev:web",
    "dev:ui": "pnpm -F ui run dev",
    "dev:web": "pnpm -F web run dev",
    "dev:infra": "pnpm -F infra run dev",
    "build:svg": "pnpm run -F icons build:svg",
    "format": "prettier **/*.{js,ts,jsx,tsx} --write --ignore-path .gitignore && git update-index --again"
  },
  "postinstall": "pnpm run build:svg",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "lint-staged": "^15.3.0",
    "prettier": "^3.2.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.3.0"
  },
   "turbo": {
    "npmClient": "pnpm",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".umi/**",
          ".next/**"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ]
      },
      "dev": {
        "cache": false
      }
    }
  }
}
